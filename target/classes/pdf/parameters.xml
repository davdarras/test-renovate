<?xml version="1.0" encoding="UTF-8"?>
<Parameters>
	<!-- The list of languages to be charged in the generated form can be defined 
		here. If this element is missing, the xml:lang attributes of the DDI file 
		are used instead. -->
	<Languages>
		<Language>fr</Language>
		<Language>en</Language>
	</Languages>
	<StudyUnit>default</StudyUnit>
	<Format>
		<Columns>1</Columns>
		<!-- Orientation : portrait= 0 ou paysage =90 -->
		<Orientation>0</Orientation>
	</Format>
	<Capture>
		<!-- manual or optical -->
		<Numeric>optical</Numeric>
	</Capture>
	<ColtraneQuestions>
		<Fin />
	</ColtraneQuestions>
	<AccompanyingMails>
		<!--<AccompanyingMail>ouvertureCOL</AccompanyingMail> -->
	</AccompanyingMails>
	 <EndQuestion>
      <ResponseTimeQuestion>true</ResponseTimeQuestion>
      <CommentQuestion>true</CommentQuestion>
    </EndQuestion>
	<Title>
		<Browsing>module</Browsing>
		<Sequence>
			<Level name="template" />
			<Level name="module">
				<PreSeq />
				<NumParent>N</NumParent>
				<PostNumParentSeq />
				<StyleNumSeq>I</StyleNumSeq>
				<PostNumSeq> - </PostNumSeq>
			</Level>
			<Level name="submodule" />
		</Sequence>
		<Question>
			<Level name="module">
				<PreQuest>&#x27A1; </PreQuest>
				<NumParent>N</NumParent>
				<PostNumParentQuest />
				<StyleNumQuest>1</StyleNumQuest>
				<PostNumQuest>. </PostNumQuest>
			</Level>
			<Level name="submodule">
				<PreQuest>&#x27A1; </PreQuest>
				<NumParent>N</NumParent>
				<PostNumParentQuest />
				<StyleNumQuest>1</StyleNumQuest>
				<PostNumQuest>. </PostNumQuest>
			</Level>
		</Question>
	</Title>
</Parameters>